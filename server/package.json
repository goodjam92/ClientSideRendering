{
  "name": "serversidetest",
  "version": "1.0.0",
  "main": "server.ts",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "server": "nodemon server.ts",
    "client": "cd ../client && yarn start",
    "start": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\" \"ts-node ./server.ts\""
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
